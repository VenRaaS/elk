                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "goods_img_url":{
                    "type":"string",
                    "index": "no"                    
                },
                "availability":{
                    "type":"integer"
                },
                "currency":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "sale_price":{
                    "type":"double"
                },
                "provider":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "barcode_ean13":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "barcode_upc":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "first_rts_date":{
                    "type":"date",
                    "index": "no",
                    "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                    "numeric_resolution":"hours"
                },
                "update_time":{
                    "type":"date",
                    "index": "no",
                    "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                    "numeric_resolution":"hours"
                }
            }
