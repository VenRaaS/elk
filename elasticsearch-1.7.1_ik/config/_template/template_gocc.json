https://www.elastic.co/guide/en/elasticsearch/reference/1.7/mapping-core-types.html

PUT _template/template_gocc

{
    "template":"*_gocc_*",
    "settings": {
        "number_of_shards": 1
    },
    "mappings":{
        "_default_":{
            "_all":{
                "enabled":false
            },
            "_ttl":{
                "enabled":true,
                "default":"14d"
            },
            "dynamic_templates":[
                {
                    "str_ik":{
                        "match":"*_ik",
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"string",
                            "index":"analyzed",
                            "analyzer":"ik"
                        }
                    }
                },
                {
                    "str_default":{
                        "match":"*_s",
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"string",
                            "index":"analyzed"
                        }
                    }
                },
                {
                    "str_na":{
                        "match":"*",
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"string",
                            "index":"not_analyzed"
                        }
                    }
                }
            ]
        },
        "category":{            
            "properties":{                
                "category_name":{
                    "type":"string",
                    "index":"analyzed",
                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "p_category_code":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "category_code":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "le":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "update_time":{
                    "type":"date",
                    "index": "no",
                    "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                    "numeric_resolution":"hours"                    
                }
            }
        },
        "goods":{
            "properties":{
                "gid":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "pgid":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "goods_name":{
                    "type":"string",
                    "index":"analyzed",
                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "goods_keyword":{
                    "type":"string",
                    "index":"analyzed",
                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "goods_brand":{
                    "type":"string",
                    "index":"analyzed",
                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "goods_describe":{
                    "type":"string",
                    "index":"analyzed",
                    "analyzer":"ik",
                    "term_vector":"with_positions_offsets"
                },
                "goods_spec":{
