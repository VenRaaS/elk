{  
    "template_bat":{  
        "template":"*_bat",
        "mappings":{  
            "_default_":{  
                "_all":{  
                    "enabled":false
                },
                "_ttl":{  
                    "enabled":true,
                    "default":"90d"
                },
                "dynamic_templates":[  
                    {  
                        "str_ik":{  
                            "match":"*_ik",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"analyzed",
                                "analyzer":"ik"
                            }
                        }
                    },
                    {  
                        "str_default":{  
                            "match":"*_s",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"analyzed"
                            }
                        }
                    },
                    {  
                        "str_na":{  
                            "match":"*",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"not_analyzed"
                            }
                        }
                    }
                ]
            },
            "category":{  
                "properties":{  
                    "category_name":{  
                        "type":"string",
						"index":"analyzed",
						"analyzer":"ik",                        
						"term_vector":"with_positions_offsets"
                    },
					"p_category_code":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "category_code":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "le":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },                    
                    "update_time":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },			
            "goods":{  
                "_id":{  
                    "index":"not_analyzed",
                    "store":true,
                    "path":"gid"
                },
                "properties":{  
                    "gid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "pgid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "goods_name":{  
                        "type":"string",
                        "index":"analyzed",
                        "analyzer":"ik",                        
                        "term_vector":"with_positions_offsets"
                    },
                    "goods_keyword":{  
                        "type":"string",
                        "index":"analyzed",
                        "analyzer":"ik",                        
                        "term_vector":"with_positions_offsets"
                    },
                    "goods_brand":{  
                        "type":"string",
                        "index":"analyzed",
                        "analyzer":"ik",                        
                        "term_vector":"with_positions_offsets"
                    },
                    "goods_describe":{  
                        "type":"string",
                        "index":"analyzed",
                        "analyzer":"ik",                        
                        "term_vector":"with_positions_offsets"
                    },
                    "goods_spec":{  
                        "type":"string",
                        "index":"analyzed",
                        "analyzer":"ik",                        
                        "term_vector":"with_positions_offsets"
                    },
                    "goods_img_url":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "availability":{  
                        "type":"integer"
                    },
                    "currency":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "sale_price":{  
                        "type":"double"
                    },
                    "provider":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "barcode_ean13":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "barcode_upc":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "first_rts_date":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    },
                    "update_time":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
            "goodscatecode":{  
                "properties":{  
                    "gid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "category_code":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "le":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "sort":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "func_type":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
            "orderlist":{
                "properties":{  
                    "uid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "order_no":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "seq":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "order_date":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"minutes"
                    },
                    "gid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "currency":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "sale_price":{  
                        "type":"double"
                    },
                    "final_price":{  
                        "type":"double"
                    },
                    "qty":{  
                        "type":"integer"
                    },
                    "final_amt":{  
                        "type":"double"
                    },
                    "promo_id":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "dc_price":{  
                        "type":"double"
                    },
                    "delivery_type":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
            "customer":{  
                "properties":{  
                    "uid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "gender":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "b_date":{  
                        "type":"date",
                        "format":"dateOptionalTime",
                        "numeric_resolution":"hours"
                    },
                    "country":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "area_1":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "area_2":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format": "yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            }
        }
		
		
    }
}
