{  
    "template_model":{  
        "template":"*_mod",
        "mappings":{  
            "_default_":{  
                "_all":{  
                    "enabled":false
                },
                "dynamic_templates":[  
                    {  
                        "str_ik":{  
                            "match":"*_ik",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"analyzed",
                                "analyzer":"ik"
                            }
                        }
                    },
                    {  
                        "str_default":{  
                            "match":"*_s",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"analyzed"
                            }
                        }
                    },
                    {  
                        "str_na":{  
                            "match":"*",
                            "match_mapping_type":"string",
                            "mapping":{  
                                "type":"string",
                                "index":"not_analyzed"
                            }
                        }
                    }
                ]
            },
			
            "tp":{  
                "_id":{  
                    "path":"category_code"
                },
                "_ttl":{  
                    "enabled":true,
                    "default":"90d"
                },
                "properties":{  
                    "category_code":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "gids":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "src":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
			
            "ar":{  
                "_id":{  
                    "path":"gid"
                },
                "_ttl":{  
                    "enabled":true,
                    "default":"90d"
                },
                "properties":{  
                    "gid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "gids":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "gids_raw":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "src":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
			
            "cooc":{  
                "_id":{  
                    "path":"gid"
                },
                "_ttl":{  
                    "enabled":true,
                    "default":"90d"
                },
                "properties":{  
                    "gid":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "Indicators":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "Indicators_raw":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "category_codes":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "src":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            },
			
            "ar_c2c":{  
                "_id":{  
                    "path":"category_code"
                },
                "_ttl":{  
                    "enabled":true,
                    "default":"90d"
                },
                "properties":{  
                    "category_code":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "category_codes":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "category_codes_raw":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "src":{  
                        "type":"string",
                        "index":"not_analyzed"
                    },
                    "update_time":{  
                        "type":"date",
                        "format":"yyyy-MM-dd HH:mm:ss || dateOptionalTime",
                        "numeric_resolution":"hours"
                    }
                }
            }
			
			
        }
    }
}