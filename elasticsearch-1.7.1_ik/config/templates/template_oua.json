{
    "template_oua" : {
        "template" : "*_oua",
        "mappings" : {
		"_default_" : {
			"_all" : {
				"enabled" : false
			},
			"dynamic_templates":[ 
			    {
				"str_ik":{
				    "match":"*_ik",
				    "match_mapping_type":"string",
				    "mapping":{
					"type":"string",
					"index":"analyzed",
					"analyzer":"ik"
				    }
				}
			    },
			    {
				"str_default":{
				    "match":"*_s",
				    "match_mapping_type":"string",
				    "mapping":{
					"type":"string",
					"index":"analyzed"
				    }
				}
			    },
			    {
				"str_na":{
				    "match":"*",
				    "match_mapping_type":"string",
				    "mapping":{
					"type":"string",
					"index":"not_analyzed"
				    }
				}
			    }
			]
		},

		"OnlineUserAlign":{
			"_id":{
				"index":"not_analyzed",
				"path":"guid"
			},
			"_ttl":{ 
				"enabled":true,
				"default":"90d"
			},
			"properties":{  
				"uid":{  
					"type":"string",
					"index":"not_analyzed"
				},
				"guid":{  
					"type":"string",
					"index":"not_analyzed"
				},
				"device":{  
					"type":"string",
					"index":"not_analyzed"
				},
				"type":{
					"type":"string",
					"index":"not_analyzed"
				},
				"insert_dt":{  
					"type":"date",
					"index":"no",
					"format":"yyyy-MM-dd HH:mm:ss||dateOptionalTime",
					"numeric_resolution":"seconds"
				},
				"insert_min_dt":{  
					"type":"date",
					"format":"yyyy-MM-dd HH:mm:ss||dateOptionalTime",
					"numeric_resolution":"minutes"
				}
			}
		}

        }
    }
}
