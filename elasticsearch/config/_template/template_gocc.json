PUT _template/template_gocc

{
    "template":"*_gocc*",
    "settings":{
        "index":{
            "number_of_shards":"1",
            "auto_expand_replicas":"0-5",
            "refresh_interval":"5s",
        }
    },
    "mappings":{
        "_default_":{
            "_all":{
                "enabled":false
            },
            "dynamic_templates":[
                {
                    "update_datetime":{
                        "match_pattern": "regex",
                        "match":"^(update_dt)|(update_time)$",
                        "mapping":{
                            "type":"date",
                            "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                        }
                    }
                },
                {
                    "str_ik":{
                        "match":"*_ik",
                        "mapping":{
                            "type":"text",
                            "analyzer":"ik_max_word",
                            "search_analyzer":"ik_max_word"
                        }
                    }
                },
                {
                    "str_text":{
                        "match":"*_t",
                        "mapping":{
                            "type":"text"
                        }
                    }
                },                
                {
                    "str_default":{
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"keyword"                            
                        }
                    }
                }
            ]
        },
        "goods":{
            "properties":{
                "gid":{
                    "type":"keyword"
                },
                "pgid":{
                    "type":"keyword"
                },
                "goods_name":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "goods_keyword":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "goods_brand":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "goods_describe":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "goods_spec":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "goods_img_url":{
                    "type":"keyword",
                    "index":false
                },
                "goods_page_url":{
                    "type":"keyword",
                    "index":false
                },
                "availability":{
                    "type":"integer"
                },
                "sale_price":{
                    "type":"scaled_float",
                    "scaling_factor":1000
                },
                "first_rts_date":{
                    "type":"date",
                    "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                },
                "sys_update_time":{
                    "type":"date",
                    "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                }
            }
        },
        "orderlist":{
            "properties":{
                "uid":{
                    "type":"keyword"
                },
                "order_no":{
                    "type":"keyword"
                },
                "seq":{
                    "type":"keyword"
                },
                "order_date":{
                    "type":"date",
                    "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                },
                "gid":{
                    "type":"keyword"
                },
                "currency":{
                    "type":"keyword"
                },
                "sale_price":{
                    "type":"scaled_float",
                    "scaling_factor":1000
                },
                "final_price":{
                    "type":"scaled_float",
                    "scaling_factor":1000
                },
                "qty":{
                    "type":"integer"
                },
                "final_amt":{
                    "type":"scaled_float",
                    "scaling_factor":1000
                },
                "promo_id":{
                    "type":"keyword"
                },
                "affiliate_id":{
                    "type":"keyword"
                },
                "dc_price":{
                    "type":"scaled_float",
                    "scaling_factor":1000
                },
                "delivery_type":{
                    "type":"keyword"
                }
            }
        },
        "group_feat_rank":{
            "properties":{
                "rank":{
                    "type":"keyword",
                    "index":false
                }
            }
        },
        "group2gid":{
            "properties":{
                "group_id":{
                    "type":"keyword"
                },
                "gid":{
                    "type":"keyword"
                }
            }
        },
        "customer":{
            "properties":{
                "uid":{
                    "type":"keyword"
                },
                "gender":{
                    "type":"keyword"
                },
                "b_date":{
                    "type":"date",
                    "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                },
                "country":{
                    "type":"keyword"
                },
                "area_1":{
                    "type":"keyword"
                },
                "area_2":{
                    "type":"keyword"
                }
            }
        },
        "goodscatecode":{
            "properties":{
                "gid":{
                    "type":"keyword"
                },
                "category_code":{
                    "type":"keyword"
                },
                "le":{
                    "type":"keyword"
                },
                "sort":{
                    "type":"scaled_float",
                    "scaling_factor":10
                },
                "func_type":{
                    "type":"keyword"
                }
            }
        },
        "category":{
            "properties":{
                "category_name":{
                    "type":"text",
                    "analyzer":"ik_max_word",
                    "search_analyzer":"ik_max_word",
                    "term_vector":"with_positions_offsets"
                },
                "p_category_code":{
                    "type":"keyword"
                },
                "category_code":{
                    "type":"keyword"
                },
                "le":{
                    "type":"keyword"
                }
            }
        },
        "group2category":{
            "properties":{
                "group_id":{
                    "type":"keyword"
                },
                "category_code":{
                    "type":"keyword"
                }
            }
        },
        "group_sort":{
            "properties":{
                "group_id":{
                    "type":"keyword"
                },
                "group_name":{
                    "type":"keyword"
                },
                "sort":{
                    "type":"scaled_float",
                    "scaling_factor":10
                }
            }
        }
    }
}
