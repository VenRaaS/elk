PUT _template/template_opp

{
  "template" : "*_opp_*",
  "mappings" : {
    "_default_" : {
      "_all" : {
          "enabled" : false
      },      
      "dynamic_templates": [
        {
          "str_ik": {
            "match": "*_ik",            
            "mapping": {
              "type": "text",
              "analyzer": "ik_max_word",
              "search_analyzer": "ik_max_word"
            }
          }
        },
        {
          "str_default": {
            "match": "*_s",            
            "mapping": {
              "type": "text"
            }
          }
        },
        {
          "str_keyword": {
            "match": "*",
            "match_mapping_type": "keyword",
            "mapping": {
              "type": "keyword"
            }
          }
        }
      ]
    },
    "OnlinePref": {
      "properties":{
        "action":{
          "type":"keyword"
        },
        "uid":{
          "type":"keyword"
        },
        "ven_guid":{
          "type":"keyword"
        },
        "ven_session":{
          "type":"keyword"
        },
        "cc_guid":{
          "type":"keyword"
        },
        "cc_session":{
          "type":"keyword"
        },
        "device":{
          "type":"keyword"
        },    
        "gid":{
          "type":"keyword"
        },            
        "category_code":{
          "type":"keyword"
        },
        "category_code_le":{
          "type":"keyword"
        },
        "client_ip":{
          "type":"ip"
        },
        "page_type":{
          "type":"keyword"
        },
        "uri":{
          "type":"keyword",
          "index":false
        },
        "referrer":{
          "type":"keyword",
          "index":false
        },
        "referrer_host":{
          "type":"keyword"
        },
        "geo_location":{
          "type" : "geo_point"
        },
        "insert_dt":{
          "type":"date",
          "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
        }
      }
    }
  }
}

