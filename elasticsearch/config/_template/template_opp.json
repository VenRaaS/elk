PUT _template/template_opp

{
    "template":"*_opp_*",
    "settings":{
        "index":{
            "number_of_shards":"3"
        }
    },
    "mappings":{
        "_default_":{
            "_all":{
                "enabled":false
            },
            "dynamic_templates":[
                {
                    "update_datetime":{
                        "match_pattern":"regex",
                        "match":"^(insert_dt)|(update_dt)|(update_time)$",
                        "mapping":{
                            "type":"date",
                            "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                        }
                    }
                },
                {
                    "str_ik":{
                        "match":"*_ik",
                        "mapping":{
                            "type":"text",
                            "analyzer":"ik_max_word",
                            "search_analyzer":"ik_max_word"
                        }
                    }
                },
                {
                    "str_text":{
                        "match":"*_t",
                        "mapping":{
                            "type":"text"
                        }
                    }
                },
                {
                    "str_default":{
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"keyword"                            
                        }
                    }
                }
            ]
        },
        "OnlinePref":{
            "properties":{
                "action":{
                    "type":"keyword"
                },
                "uid":{
                    "type":"keyword"
                },
                "ven_guid":{
                    "type":"keyword"
                },
                "ven_session":{
                    "type":"keyword"
                },
                "device":{
                    "type":"keyword"
                },
                "gid":{
                    "type":"keyword"
                },
                "category_code":{
                    "type":"keyword"
                },
                "client_ip":{
                    "type":"ip"
                },
                "page_type":{
                    "type":"keyword"
                },
                "geo_location":{
                    "type":"geo_point"
                }
            }
        },
        "user_prefer_goods":{
            "properties":{
                "w_list_type":{
                    "type":"keyword"
                },
                "uid":{
                    "type":"keyword"
                },
                "categ_code":{
                    "type":"keyword"
                },
                "gid,":{
                    "type":"keyword"
                }
            }
        }
    }
}
