PUT _template/template_oua

{
    "template":"*_oua_*",
    "settings":{
        "index":{
            "number_of_shards":"1"
        }
    },
    "mappings":{
        "_default_":{
            "_all":{
                "enabled":false
            },
            "dynamic_templates":[
                {
                    "datetime":{
                        "match_pattern":"regex",
                        "match":"^(insert_dt)|(update_dt)|(update_time)$",
                        "mapping":{
                            "type":"date",
                            "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                        }
                    }
                },
                {
                    "datetime_min":{
                        "match":"insert_min_dt",
                        "mapping":{
                            "type":"date",
                            "format":"date_hour_minute"
                        }
                    }
                },
                {
                    "str_ik":{
                        "match":"*_ik",
                        "mapping":{
                            "type":"text",
                            "analyzer":"ik_max_word",
                            "search_analyzer":"ik_max_word"
                        }
                    }
                },
                {
                    "str_text":{
                        "match":"*_t",
                        "mapping":{
                            "type":"text"
                        }
                    }
                },
                {
                    "str_default":{
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"keyword"
                        }
                    }
                }
            ]
        },
        "OnlineUserAlign":{
            "dynamic": false,
            "properties":{
                "uid":{
                    "type":"keyword"
                },
                "ven_guid":{
                    "type":"keyword"
                },
                "device":{
                    "type":"keyword",
                    "index":false
                }
            }
        }
    }
}
