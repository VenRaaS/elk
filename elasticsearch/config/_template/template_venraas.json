PUT _template/template_venraas

{
    "template":"venraas",
    "settings":{
        "index":{
            "number_of_shards":"1"
        }
    },
    "mappings":{
        "_default_":{
            "_all":{
                "enabled":false
            },
            "dynamic_templates":[
                {
                    "update_datetime":{
                        "match_pattern":"regex",
                        "match":"^(insert_dt)|(update_dt)|(update_time)$",
                        "mapping":{
                            "type":"date",
                            "format":"epoch_second || yyyy-MM-dd HH:mm:ss || yyyy-MM-dd'T'HH:mm:ss"
                        }
                    }
                },
                {
                    "str_ik":{
                        "match":"*_ik",
                        "mapping":{
                            "type":"text",
                            "analyzer":"ik_max_word",
                            "search_analyzer":"ik_max_word"
                        }
                    }
                },
                {
                    "str_text":{
                        "match":"*_t",
                        "mapping":{
                            "type":"text"
                        }
                    }
                },
                {
                    "str_default":{
                        "match_mapping_type":"string",
                        "mapping":{
                            "type":"keyword"
                        }
                    }
                }
            ]
        },
        "com_pkgs":{
            "properties":{
                "account":{
                    "type":"keyword"
                },
                "account_id":{
                    "type":"long"
                },
                "companies":{
                    "properties":{
                        "code_name":{
                            "type":"keyword"
                        },
                        "domain_name":{
                            "type":"keyword"
                        },
                        "enabled":{
                            "type":"boolean"
                        },
                        "evaluation_date_range":{
                            "type":"long"
                        },
                        "id":{
                            "type":"long"
                        },
                        "name":{
                            "type":"text",
                            "analyzer":"ik_max_word",
                            "search_analyzer":"ik_max_word"
                        },
                        "offline_model_status":{
                            "type":"boolean"
                        },
                        "token":{
                            "type":"keyword"
                        },
                        "venraas_pt_status":{
                            "type":"boolean"
                        }
                    }
                }
            }
        }
    }
}
